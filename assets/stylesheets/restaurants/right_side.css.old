.restoran-item {
    border: 1px solid #eee;
    padding: 25px 20px;
    margin-bottom: 20px;
    display: block;
    text-decoration: none;
    background: #fff;
    -webkit-transition: border .5s ease;
    -moz-transition: border .5s ease;
    transition: border .5s ease;
    position: relative
}
.restoran-item:hover {
    border: 1px solid #c7c7c7
}
a.restoran-item:hover .restoran-item_title {
    color: #669cd6
}
.restoran-item_closed {
    font-size: 11px;
    line-height: 19px;
    color: #ffa115;
    font-weight: 800;
    margin-bottom: 20px
}
.restoran-item_closed i {
    display: inline-block;
    vertical-align: top;
    margin-right: 7px
}
.restoran-item_closed span {
    border-bottom: 1px dashed #ffa115
}
.restoran-item_image img {
    display: block;
    margin: 0 auto;
    max-width: 100%;
    height: auto
}
.restoran-item--user:hover {
    border: 1px solid #eee
}
.restoran-item--user .restoran-item_image {
    max-width: 200px;
    margin: 0 auto
}
.restoran-item_top {
    padding-bottom: 25px;
    border-bottom: 1px solid #eee
}
.restoran-item_title {
    font-size: 22px;
    line-height: 26px;
    font-family: "proxima", "Arial", sans-serif;
    color: #333;
    font-weight: 700;
    margin-bottom: 15px;
    -webkit-transition: color .4s ease;
    -moz-transition: color .4s ease;
    transition: color .4s ease
}
.restoran-item_description {
    font-size: 13px;
    line-height: 20px;
    color: #333;
    font-weight: 400
}
.restoran-item_description span {
    font-weight: 700;
    color: #959595
}
.restoran-item_description span.distance {
    font-weight: normal;
    margin-left: 30px;
    position: relative;
    color: #a3a3a3
}
.restoran-item_description span.distance:before {
    content: '';
    width: 12px;
    height: 14px;
    position: absolute;
    display: block;
    top: 2px;
    left: -16px;
    background: url(../../images/sprite.png) no-repeat -685px -448px
}
.restoran-item_star-col {
    font-size: 11px;
    line-height: 16px;
    color: #9d9d9d;
    font-weight: 400;
    display: block
}
.restoran-item_bottom {
    margin-top: 13px
}
.restoran-item_bottom .col {
    padding: 0 5px
}
.restoran-item_sub-titile {
    font-size: 10px;
    text-transform: uppercase;
    color: #959595;
    line-height: 14px;
    margin-bottom: 10px
}
.restoran-item_big {
    font-size: 17px;
    line-height: 18px;
    color: #000;
    font-weight: 300
}
.restoran-item_big i {
    display: inline-block;
    vertical-align: middle;
    margin-right: 5px
}
.restoran-item--close>.s-9 {
