.box-tooltip {
    display: inline-block
}
.b-tooltip {
    display: none;
    font-family: Arial, Verdana, Tahoma, sans-serif;
    font-size: 12px;
    font-style: normal;
    font-weight: normal;
    background: #FFF;
    position: absolute;
    width: 220px;
    pointer-events: none;
    padding: 15px;
    color: #000;
    text-align: left;
    line-height: 14px;
    z-index: 100;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    -webkit-box-shadow: rgba(0, 0, 0, 0.27) 0 1px 9px;
    -moz-box-shadow: rgba(0, 0, 0, 0.27) 0 1px 9px;
    box-shadow: rgba(0, 0, 0, 0.27) 0 1px 9px
}
.b-tooltip.tc {
    bottom: 150%;
    left: 50%;
    margin-left: -110px
}
.b-tooltip.tl {
    bottom: 150%;
    left: -70px
}
.b-tooltip.to {
    bottom: 110%;
    left: 0;
    opacity: 0;
    transition: all .5s ease;
    display: block
}
.b-tooltip.th {
    bottom: 120%;
    left: 50%;
    margin-left: -110px
}
.b-tooltip:before {
    content: '';
    background: url(../../images/angle.png) no-repeat 0 -23px;
    width: 50px;
    height: 23px;
    display: inline-block;
    top: 100%;
    left: 50%;
    margin-left: -25px;
    position: absolute
}
.b-tooltip.tg:before {
    content: '';
    background: url(../../images/angle.png) no-repeat 23px -10px;
    width: 42px;
    height: 25px;
    display: inline-block;
    top: 20px;
    left: 0;
    margin-left: -42px;
    position: absolute
}
.b-tooltip.tg {
    top: 0;
    left: 100%;
    margin-top: -25px
}